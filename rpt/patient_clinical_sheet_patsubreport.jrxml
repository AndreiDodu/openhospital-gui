	<parameter name="patientID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT *
FROM PATIENT
LEFT JOIN (SELECT PEX_PAT_ID, PEX_HEIGHT, PEX_WEIGHT FROM PATIENTEXAMINATION WHERE PEX_PAT_ID = $P{patientID} ORDER BY PEX_DATE DESC LIMIT 1) AS HW ON PAT_ID = PEX_PAT_ID
LEFT JOIN PATIENT_PROFILE_PHOTO ON PROFILE_PHOTO_ID = PAT_PROFILE_PHOTO_ID
WHERE PAT_ID = $P{patientID}]]>
	</queryString>
